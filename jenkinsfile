pipeline {
    agent any
     stages{
     
      stage('build'){
       steps {
        sh "mvn clean"
        sh "mvn install"
        sh "mvn package"
        sleep 10
         echo "Hello World"
       }
    }
     stage('deploy'){
       steps {
         sleep 10
          echo "Hello World"
       } 
    }
     stage('Hel'){
       steps {
         echo "Hello World"
       }
    }
    
    }

}
